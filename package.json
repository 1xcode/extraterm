{
  "name": "extraterm",
  "version": "0.1.0",
  "description": "A terminal",
  "author": "Simon Edwards",
  "license": "MIT",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/sedwards2009/extraterm.git"
  },
  "scripts": {
    "run": "electron --force-device-scale-factor=1 src/main.js",
    "build": "tsc",
    "test": "npm run build ; nodeunit src/virtualscrollareatest.js",
    "typedoc": "node src/build_scripts/prepare_typedoc.js && cd src && typedoc --target ES5 --name Extraterm --mode file --module amd --out typedocs build_tmp",
    "package": "node src/build_scripts/build_packages.js",
    "serve-test": "node src/test/httpserver.js"
  },
  "dependencies": {
    "base64-arraybuffer": "0.1.5",
    "codemirror": "5.12.0",
    "he": "0.5.0",
    "immutable": "3.7.3",
    "js-base64": "2.1.8",
    "lodash": "3.9.1",
    "markdown": "0.5.0",
    "qs": "3.1.0",
    "sass.js": "0.9.7",
    "source-map-support": "0.3.2",
    "text-encoding": "0.5.5",
    "utf8": "2.1.1",
    "vue": "1.0.10"
  },
  "optionalDependencies": {
    "pty.js": "0.3.0"
  },
  "devDependencies": {
    "cajon": "0.2.10",
    "electron-packager": "github:electron-userland/electron-packager",
    "electron-prebuilt": "0.36.3",
    "finalhandler": "0.4.0",
    "nodeunit": "0.9.1",
    "serve-static": "1.10.0",
    "shelljs": "0.6.0",
    "tsd": "0.6.0-beta.5",
    "typedoc": "0.3.12",
    "typescript": "1.8.2"
  },
  "jshintConfig": {
    "esversion": 6
  }
}
