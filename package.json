{
  "name": "extraterm",
  "version": "0.31.0",
  "description": "A terminal",
  "author": "Simon Edwards",
  "license": "MIT",
  "main": "extraterm/main.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/sedwards2009/extraterm.git"
  },
  "scripts": {
    "run": "electron extraterm/src/main_process/Main.js --dev-tools",
    "build-all": "yarn run build-web-component-decorators && rm -rf node_modules/extraterm-web-component-decorators && yarn install --check-files && yarn run yarn-install-extension-api && yarn run build && yarn run build-extensions",
    "electron-rebuild": "node ./node_modules/electron-rebuild/lib/src/cli.js -o ptyw.js,font-manager -f -v 1.6.8",
    "yarn-prune-extensions": "node build_scripts/npm_extensions.js prune",
    "package": "node build_scripts/build_packages.js"
  },
  "jshintConfig": {
    "esversion": 6
  },
  "resolutions": {
    "@types/node": "7.0.52",
    "typescript": "2.6.1"
  },
  "private": true,
  "workspaces": [
    "extraterm",
    "packages/*",
    "extensions/*"
  ],
  "devDependencies": {
    "wsrun": "^1.5.1"
  }
}
