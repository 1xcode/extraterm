{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "declaration": true,
        "noImplicitAny": false,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "removeComments": false,
        "sourceMap": true,
        "noLib": false,
        "outDir": ".",
        "jsx": "react",
        "experimentalDecorators": true,
        "baseUrl": "."
      
    },
    "filesGlob": [
        "./*.ts",
        "./typings/**/*.ts",
        "!./src/node_modules/**/*.ts"
    ],
    "files": [
        "./main.ts"
    ],
    "exclude": []
}
