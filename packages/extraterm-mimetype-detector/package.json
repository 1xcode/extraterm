{
  "name": "extraterm-mimetype-detector",
  "version": "1.0.0",
  "description": "Detect a mimetype from the first few bytes of a file",
  "author": "Simon Edwards",
  "license": "MIT",
  "type": "module",
  "exports": "./dist/MimeTypeDetector.js",
  "types": "./dist/MimeTypeDetector.d.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/sedwards2009/extraterm.git"
  },
  "dependencies": {
    "file-type": "12.4.2",
    "jschardet": "1.4.1",
    "mime-types": "^2.1.21"
  },
  "devDependencies": {
    "@types/jest": "27.0.1",
    "eslint": "8.13.0",
    "eslint-config-extraterm": "1.0.0",
    "eslint-plugin-unicorn": "42.0.0",
    "jest": "27.5.1",
    "shx": "^0.3.2",
    "typescript": "4.7.4"
  },
  "scripts": {
    "build": "yarn run build-fast && yarn run lint",
