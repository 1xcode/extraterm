{
  "name": "extraterm",
  "version": "1.0.0",
  "description": "A terminal",
  "scripts": {
    "build": "tsc",
    "build-icons": "node resources/extra_icons/build.js",
    "electron-rebuild": "node ./node_modules/electron-rebuild/lib/src/cli.js -o ptyw.js,font-manager -f -v 1.6.8",
    "testextensionloader": "yarn run build && nodeunit src/render_process/extension/ExtensionLoaderTest.js",
    "testkeybindingmanager": "yarn run build && nodeunit src/render_process/keybindings/KeyBindingManagerTest.js",
    "testmimetypedetector": "yarn run build && nodeunit src/mimetype_detector/MimeTypeDetectorTest.js",
    "testsplitlayout": "yarn run build && nodeunit src/render_process/SplitLayoutTest.js",
    "testterm": "tsc && nodeunit src/render_process/emulator/TermTest.js",
    "typedoc": "node build_scripts/prepare_typedoc.js && cd src && typedoc --target ES5 --name Extraterm --mode file --module amd --out typedocs build_tmp",
    "package": "node build_scripts/build_packages.js",
    "serve-test": "node src/test/httpserver.js"
  },
  "dependencies": {
    "codemirror": "5.27.4",
    "commander": "^2.9.0",
    "element-resize-detector": "1.1.3",
    "extraterm-web-component-decorators": "file:../packages/extraterm-web-component-decorators/",
    "font-manager": "0.2.2",
    "fontinfo": "0.3.0",
    "get-uri": "2.0.1",
    "he": "0.5.0",
    "jschardet": "1.4.1",
    "lodash": "^4.0.0",
    "markdown": "0.5.0",
    "node-sass": "4.5.3",
    "qs": "6.3.1",
    "smart-buffer": "^3.0.3",
    "source-map-support": "^0.3.2",
    "utf8": "2.1.2",
    "vue": "2.5.9",
    "vue-class-component": "6.1.0"
  },
  "optionalDependencies": {
    "ptyw.js": "0.4.0"
  },
  "devDependencies": {
    "@types/commander": "2.3.31",
    "@types/electron": "1.4.32",
    "@types/he": "0.5.29",
    "@types/lodash": "^4.0.0",
    "@types/node": "7.0.52",
    "@types/node-sass": "3.10.32",
    "@types/nodeunit": "0.0.30",
    "@types/qs": "6.2.30",
    "@types/source-map-support": "0.2.28",
    "@types/utf8": "2.1.5",
    "cajon": "0.2.10",
    "electron": "1.6.8",
    "electron-packager": "8.7.0",
    "electron-rebuild": "1.7.2",
    "extraterm-extension-api": "file:../packages/extraterm-extension-api/",
    "finalhandler": "0.4.0",
    "modclean": "2.1.0",
    "nodeunit": "0.11.0",
    "performance-now": "2.1.0",
    "serve-static": "1.10.0",
    "shelljs": "0.7.8",
    "typedoc": "0.3.12",
    "typescript": "2.6.1",
    "webfont": "^4.0.0"
  },
  "jshintConfig": {
    "esversion": 6
  }
}
