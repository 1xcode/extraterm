    "commands": [
      {
        "command": "tldr-pages:tldr",
        "title": "TLDR Pages",
        "category": "terminal",
        "when": "connectedTerminalFocus"
      }
    ]
  },
  "devDependencies": {
    "@extraterm/extraterm-extension-api": "0.15.0",
    "@types/jest": "27.0.1",
    "download-cli": "^1.1.1",
    "jest": "27.5.1",
    "shelljs": "0.8.3",
    "typescript": "4.7.4"
  }
}
