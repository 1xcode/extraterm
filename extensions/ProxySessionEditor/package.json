{
  "name": "proxy-session-editor",
  "description": "Proxy based Session Editor",
  "author": "Simon Edwards",
  "license": "MIT",
  "version": "1.0.0",
  "type": "module",
  "exports": "./dist/ProxySessionEditorExtension.js",
  "scripts": {
    "build": "yarn run build-fast && yarn run lint",
    "build-fast": "tsc",
    "clean": "shx rm -rf dist",
    "lint": "eslint \"src/**/*.ts\"",
    "lint-strict": "eslint --max-warnings 1 \"src/**/*.ts\""
  },
  "dependencies": {
    "fs-extra": "^5.0.0",
    "lodash-es": "4.17.21"
  },
  "devDependencies": {
    "@extraterm/extraterm-extension-api": "0.15.0",
    "@types/fs-extra": "^5.0.2",
    "@types/lodash-es": "4.17.6",
    "@types/node": "^16.4.0",
    "eslint": "8.13.0",
    "eslint-config-extraterm": "1.0.0",
    "eslint-plugin-unicorn": "42.0.0",
    "shx": "^0.3.2",
    "typescript": "4.6.3"
  },
  "isInternal": true,
  "includePlatform": [
    {
      "os": "win32"
    }
  ],
  "contributes": {
    "sessionEditors": [
      {
        "name": "Windows Subsystem for Linux",
        "type": "wsl"
      }
    ]
  }
}
