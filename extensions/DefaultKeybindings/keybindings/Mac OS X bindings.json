{
  "name": "Mac OS X bindings",
  "main-ui": {
    "Cmd-ArrowLeft": "focusTabLeft",
    "Cmd-ArrowRight": "focusTabRight",
    "Cmd-t": "newTerminal",
    "Shift-Cmd-W": "closeTab",
    "Cmd-Shift-<": "focusPaneLeft",
    "Cmd-Shift->": "focusPaneRight",
    "Cmd-Shift-:": "focusPaneAbove",
    "Cmd-Shift-?": "focusPaneBelow",
    "Cmd-Alt-,": "moveTabLeft",
    "Cmd-Alt-.": "moveTabRight",
    "Cmd-Alt-;": "moveTabUp",
    "Cmd-Alt-/": "moveTabDown"
  },
  "terminal-default-mode": {
    "Alt-Space": "enterCursorMode",
    "Shift-PageUp": "scrollPageUp",
    "Shift-PageDown": "scrollPageDown",
    "Cmd-Shift-PageUp": "goToPreviousFrame",
    "Cmd-Shift-PageDown": "goToNextFrame",
    "Cmd-c": "copyToClipboard",
    "Cmd-v": "pasteFromClipboard",
    "Cmd-w": "deleteLastFrame",
    "Cmd-o": "openLastFrame",
    "Cmd-Shift-Plus": "increaseFontSize",
    "Cmd-Minus": "decreaseFontSize",
    "Cmd-=": "resetFontSize"
  },
  "terminal-cursor-mode": {
    "Alt-Space": "enterNormalMode",
    "Cmd-Shift-PageUp": "goToPreviousFrame",
    "Cmd-Shift-PageDown": "goToNextFrame",
    "Cmd-c": "copyToClipboard",
    "Cmd-v": "pasteFromClipboard",
    "Cmd-Shift-Plus": "increaseFontSize",
    "Cmd-Minus": "decreaseFontSize",
    "Cmd-=": "resetFontSize"
  },
  "editor": {
    "Left": "gotoLeft",
    "Right": "gotoRight",
    "Up": "goLineUp",
    "Down": "goLineDown",
    "End": "gotoLineEnd",
    "Home": "gotoLineStart",
    "PageUp": "gotoPageUp",
    "PageDown": "gotoPageDown",
    "Delete": "del",
    "Backspace": "backspace",
    "Shift-Backspace": "backspace",
    "Shift-Up": "selectUp",
    "Shift-Down": "selectDown",
    "Shift-Home": "selectLineStart",
    "Shift-End": "selectLineEnd",    
    "Shift-Left": "selectLeft",
    "Shift-Right": "selectRight",
    "Shift-PageUp": "selectPageUp",
    "Shift-PageDown": "selectPageDown",
    "Cmd-Shift-Left": "selectWordLeft",
    "Cmd-Shift-Right": "selectWordRight",
    "Ctrl-Shift-Up": "addCursorAbove",
    "Alt-Shift-Up": "addCursorAbove",
    "Ctrl-Shift-Down": "addCursorBelow",
    "Alt-Shift-Down": "addCursorBelow",
    "Alt-Shift-I": "splitIntoLines",
    "Ctrl-F2": "findAll",
    "Ctrl-d": "selectMoreAfter",
    "Tab": "indent",
    "Shift-Tab": "outdent",
    "Insert": "overwrite",
    "Cmd-a": "selectAll",
    "Cmd-d": "removeLines",
    "Cmd-z": "undo",
    "Shift-Cmd-Z": "redo",
    "Esc": "singleSelection",
    "Cmd-y": "redo",
    "Cmd-Home": "gotoFileStart",
    "Cmd-End": "gotoFileEnd",
    "Cmd-Up": "scrollUp",
    "Cmd-Down": "scrollDown",
    "Alt-Left": "gotoWordLeft",
    "Alt-Right": "gotoWordRight",
    "Alt-Backspace": "removeWordLeft",
    "Ctrl-Alt-Backspace": "removeWordRight",
    "Alt-Delete": "removeWordRight",
    "Cmd-[": "blockOutdent",
    "Cmd-]": "blockIndent",
    "Cmd-U": "undoSelection",
    "Shift-Cmd-U": "redoSelection",
    "Cmd-Shift-|": "cursorColumn",
    "Cmd-/": "backSlashesToForward",
    "Cmd-\\": "forwardSlashesToBack",
    "Cmd-'": "escapeShellChars",
    "Cmd-Shift-\"": "unescapeShellChars",
    "Cmd-Left": "goLineLeft",
    "Cmd-Right": "goLineRight",
    "Cmd-Backspace": "delWrappedLineLeft",
    "Cmd-Delete": "delWrappedLineRight"
