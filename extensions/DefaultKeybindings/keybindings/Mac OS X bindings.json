  "terminal-viewer-default-mode": {
    "Cmd-Enter": "typeSelection",
    "Cmd-Shift-Enter": "typeSelectionAndCr",
    "Cmd-Shift-P": "openCommandPalette"
  },
  "terminal-viewer-cursor-mode": {
    "Cmd-Enter": "typeSelection",
    "Cmd-Shift-Enter": "typeSelectionAndCr",
    "Cmd-Shift-P": "openCommandPalette",
    "fallthrough": "editor"
