{
  "extends": "pc-style",
  "bindings": [
    {
      "command": "extraterm:window.focusTabLeft",
      "keys": [
        "Ctrl-,"
      ]
    },
    {
      "command": "extraterm:window.focusTabRight",
      "keys": [
        "Ctrl-."
      ]
    },
    {
      "command": "extraterm:window.newTerminal",
      "keys": [
        "Ctrl-Shift-T"
      ]
    },
    {
      "command": "extraterm:window.closeTab",
      "keys": [
        "Ctrl-Shift-Q"
      ]
    },
    {
      "command": "extraterm:terminal.scrollPageUp",
      "keys": [
        "Shift-PageUp"
      ]
    },
    {
      "command": "extraterm:terminal.scrollPageDown",
      "keys": [
        "Shift-PageDown"
      ]
    },
    {
      "command": "extraterm:terminal.goToPreviousFrame",
      "keys": [
        "Ctrl-Shift-PageUp"
      ]
    },
    {
      "command": "extraterm:terminal.goToNextFrame",
      "keys": [
        "Ctrl-Shift-PageDown"
      ]
    },
    {
      "command": "extraterm:terminal.copyToClipboard",
      "keys": [
        "Ctrl-Shift-C"
      ]
    },
    {
      "command": "extraterm:terminal.pasteFromClipboard",
      "keys": [
        "Shift-Insert",
        "Ctrl-Shift-V"
      ]
    },
    {
      "command": "extraterm:terminal.deleteLastFrame",
      "keys": [
        "Ctrl-Shift-W"
      ]
    },
    {
      "command": "extraterm:terminal.openLastFrame",
      "keys": [
        "Ctrl-Shift-O"
      ]
    },
    {
      "command": "extraterm:terminal.increaseFontSize",
      "keys": [
        "Ctrl-Shift-Plus"
      ]
    },
    {
      "command": "extraterm:terminal.decreaseFontSize",
      "keys": [
        "Ctrl-Minus"
      ]
    },
    {
      "command": "extraterm:terminal.resetFontSize",
      "keys": [
        "Ctrl-="
      ]
    },
    {
      "command": "extraterm:viewerTab.copyToClipboard",
      "keys": [
        "Ctrl-c"
      ]
    },
    {
      "command": "extraterm:viewerTab.pasteFromClipboard",
      "keys": [
        "Shift-Insert",
        "Ctrl-v"
      ]
    },
    {
      "command": "extraterm:application.openCommandPalette",
      "keys": [
        "Ctrl-Shift-P"
      ]
    },
    {
      "command": "extraterm:application.newWindow",
      "keys": [
        "Ctrl-Shift-N"
      ]
    }
  ]
}
