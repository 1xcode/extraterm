{
  "name": "itermcolors-terminal-theme-provider",
  "displayName": "iTerm Color Theme Support",
  "description": "Adds support for iTerm format terminal color themes.",
  "author": "Simon Edwards",
  "license": "MIT",
  "version": "1.0.0",
  "type": "module",
  "exports": "./dist/ITermColorsTerminalThemeProviderExtension.js",
  "isInternal": true,
  "scripts": {
    "build": "yarn run build-fast && yarn run lint",
    "build-fast": "tsc",
    "clean": "shx rm -rf dist",
    "lint": "eslint \"src/**/*.ts\"",
    "lint-strict": "eslint --max-warnings 1 \"src/**/*.ts\"",
    "test": "yarn run build && yarn node --experimental-vm-modules $(yarn bin jest)"
  },
  "dependencies": {
    "extraterm-logging": "1.0.0",
    "plist": "3.0.5"
  },
  "devDependencies": {
    "@extraterm/extraterm-extension-api": "0.15.0",
    "@types/jest": "27.0.1",
    "@types/plist": "^3.0.2",
    "eslint": "8.13.0",
    "eslint-config-extraterm": "1.0.0",
    "eslint-plugin-unicorn": "42.0.0",
    "jest": "27.5.1",
    "shx": "^0.3.2",
    "typescript": "4.7.4"
  },
  "contributes": {
    "terminalThemeProviders": [
      {
        "name": "iTermColors",
        "humanFormatNames": [
          "iTerm2 .itermcolors"
        ]
      }
    ]
  }
}
